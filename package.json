{
	"name": "gradebook",
	"version": "0.0.1",
	"scripts": {
		"dev": "dotenv -e .env.development -- vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "vitest",
		"prod": "dotenv -e .env.production -- node build",
		"coverage": "vitest run --coverage",
		"prisma-setup-dev": "dotenv -e .env.development -- prisma generate && dotenv -e .env.development -- prisma db push && dotenv -e .env.development -- prisma db seed",
		"prisma-setup": "dotenv -e .env.production -- prisma generate"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.85",
		"@sveltejs/kit": "1.0.0-next.480",
		"@types/node": "^18.6.3",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"@types/cookie": "^0.5.1",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "^0.5.13",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0",
		"vitest": "^0.20.3"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.1.1",
		"dotenv-cli": "^6.0.0",
		"prisma": "^4.1.1",
		"ts-node": "^10.9.1",
		"zod": "^3.17.10",
		"zod-form-data": "^1.2.1"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm ./prisma/seed.ts"
	}
}
